      "hideEmpty": false,
      "hideZero": false,
      "id": 4,
      "initialZoom": "4",
      "locationData": "table",
      "mapCenter": "custom",
      "mapCenterLatitude": "-26.724895",
      "mapCenterLongitude": " -69.705463",
      "maxDataPoints": 1,
      "mouseWheelZoom": true,
      "options": {},
      "showLegend": true,
      "stickyLabels": false,
      "tableQueryOptions": {
        "geohashField": "geohash",
        "labelField": "tooltip",
        "latitudeField": "w_lat",
        "longitudeField": "w_long",
        "metricField": "level_percentage",
        "queryType": "coordinates"
      },
      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  m_timestamp AS \"time\",\r\n  cast(m_value as double precision) / ( well.w_diameter * well.w_maxdepth ) as level_percentage,\r\n  w_lat,\r\n  w_long,\r\n  ('<span style=\"color:black\">Water Level of Well ' || w_name || '</span>')  as tooltip\r\nFROM measurement\r\nJOIN well on m_fk_wellid = w_id\r\nJOIN sensor_type on measurement.sensor_type_st_id = sensor_type.st_id\r\nWHERE w_id = '[[w_id]]' and sensor_type.st_value = 'level'\r\nORDER BY time desc LIMIT 1",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "m_water_level"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "measurement",
          "timeColumn": "m_timestamp",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "thresholds": "0.1,0.25",
      "timeFrom": null,
      "timeShift": null,
      "title": "Well Location",
      "transparent": true,
      "type": "grafana-worldmap-panel",
      "unitPlural": "",
      "unitSingle": "",
      "unitSingular": "Water Level in Percent",
      "valueName": "total"
    },
    {
      "aliasColors": {},
      "cacheTimeout": null,
      "datasource": null,
      "description": "The fill of the well in percent",
      "doInit": {},
      "format": "short",
